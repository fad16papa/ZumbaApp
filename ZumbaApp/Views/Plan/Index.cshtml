                    <i class="icon-menu h4"></i>
                </button>
                <div class="collapse navbar-collapse justify-content-between" id="navbarNav">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a asp-controller="User" asp-action="RegisterPage">Sign up</a>
                            <span>&nbsp;or&nbsp;</span><a asp-controller="User" asp-action="Login">Sign in</a>
                        </li>
                    </ul>
                </div>
                <!--end nav collapse-->
            </nav>
        </div>
        <!--end of container-->
    </div>
</div>
